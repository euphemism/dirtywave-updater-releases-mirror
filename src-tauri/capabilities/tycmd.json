{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "tycmd",
	"description": "Capability for tycmd sidecar",
	"windows": ["*"],
	"permissions": [
		{
			"identifier": "shell:allow-execute",
			"allow": [
				{
					"args": true,
					"cmd": "tycmd",
					"name": "binaries/tycmd",
					"sidecar": true
				}
			]
		},
		"shell:allow-open"
	]
}
